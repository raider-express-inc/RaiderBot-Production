{
  "models": [
    {
      "title": "GPT-4",
      "provider": "openai",
      "model": "gpt-4",
      "apiKey": "${OPENAI_API_KEY}"
    }
  ],
  "customCommands": [
    {
      "name": "ask",
      "description": "Ask Continue.dev to scaffold new code patterns",
      "prompt": "Generate code for: {input}"
    },
    {
      "name": "foundry_scaffold",
      "description": "Scaffold Foundry Workshop applications",
      "prompt": "Create a Foundry Workshop application with: {input}"
    },
    {
      "name": "aip_tool",
      "description": "Generate AIP Studio tool functions",
      "prompt": "Create an AIP Studio tool function for: {input}"
    },
    {
      "name": "debug_foundry",
      "description": "Debug Foundry API integration issues",
      "prompt": "Debug this Foundry API issue: {input}"
    },
    {
      "name": "orchestrator_agent",
      "description": "Generate multi-agent orchestrator patterns",
      "prompt": "Create orchestrator agent for: {input}"
    },
    {
      "name": "snowflake_audit",
      "description": "Generate Snowflake audit logging code",
      "prompt": "Create Snowflake audit logging for: {input}"
    },
    {
      "name": "explain",
      "description": "Explain unfamiliar code patterns",
      "prompt": "Explain this code: {input}"
    },
    {
      "name": "refactor",
      "description": "Refactor and clean code",
      "prompt": "Refactor this code for better structure: {input}"
    },
    {
      "name": "docsearch",
      "description": "Search Foundry documentation",
      "prompt": "Find Foundry documentation for: {input}"
    },
    {
      "name": "raiderbot_deployment",
      "description": "Deploy consolidated RaiderBot system",
      "prompt": "Deploy RaiderBot with Workshop app, AIP agent, and quarterback functions to Foundry"
    }
  ],
  "includePaths": [
    "src/foundry/",
    "src/aip/",
    "server.py",
    "deployment/",
    "scripts/",
    "tests/"
  ],
  "excludePaths": [
    "tests/",
    ".git/",
    "logs/",
    "__pycache__/",
    "*.pyc"
  ],
  "contextLength": 8000,
  "completionOptions": {
    "temperature": 0.2,
    "maxTokens": 1000
  }
}